{
  "title": "paste",
  "rules": [
    {
      "description": "shift+insert - paste",
      "manipulators": [
        {
          "from": {
            "key_code": "insert",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "v",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "toggle fullscreen",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
